  "scripts": {
    "format": "yarn prettier --write ",
    "prettier": "prettier \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|mdx|graphql|vue)\"",
    "lint": "eslint \"./+(!node_modules|src|server)/!(tests)/**/*.+(ts|tsx)\" --fix",
    "validate": "yarn lint && yarn format && yarn prettier --list-different \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|mdx|graphql|vue)\"",
    "build": "next build src",
    "local": "cross-env ENVIRONMENT=local pm2-runtime start ecosystem.config.js --only books-server --env local",
    "deploy": "cross-env SLS_DEBUG=* yarn clear && yarn build && yarn clear-source-map && serverless deploy --stage $ENVIRONMENT -v",
    "offline": "cross-env SLS_DEBUG=* yarn clear && yarn build && serverless offline -v",
    "clear": "rm -rf ./build",
    "clear-source-map": "find ./build -type f -name '*.js.map' -delete",
    "test": "jest --passWithNoTests --collectCoverage --colors",
    "analyze": "BUNDLE_ANALYZE=both yarn build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    "last 2 versions",
    "Android >= 4.2",
    "iOS >= 10",
    "ie >= 11"
  ]
}
